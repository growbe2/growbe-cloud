{
  "name": "growbe-cloud",
  "version": "0.0.0",
  "description": "Cloud server for Growbe",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "npm run build && lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "docker:build": "mkdir -p ./storage && cp -r ../releases ./storage/ && docker build -f ./docker/growbe-cloud.Dockerfile -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-cloud:$VERSION --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
    "docker:push": "docker push docker.pkg.github.com/growbe2/growbe-cloud/growbe-cloud:$VERSION",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start:cloud": "node -r source-map-support/register ./dist/growbe-cloud.js",
    "start:watcher": "node -r source-map-support/register ./dist/growbe-watcher.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "William Quintal <wquintal@berlingoqc.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@berlingoqc/lb-extensions": "^1.2.0",
    "@berlingoqc/sso": "^1.1.5",
    "@growbe2/growbe-pb": "^1.3.0-SNAPSHOT-1",
    "@loopback/authentication": "^9.0.2",
    "@loopback/authorization": "^0.12.2",
    "@loopback/boot": "^5.0.2",
    "@loopback/core": "^4.0.2",
    "@loopback/health": "^0.11.2",
    "@loopback/metrics": "^0.11.2",
    "@loopback/repository": "^5.0.2",
    "@loopback/rest": "^12.0.2",
    "@loopback/rest-explorer": "^5.0.2",
    "@loopback/security": "^0.8.2",
    "@loopback/service-proxy": "^5.0.2",
    "adm-zip": "^0.5.9",
    "async-mqtt": "^2.6.2",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "dot": "^1.1.3",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^6.2.0",
    "loopback-connector-postgresql": "^5.5.1",
    "mqtt": "^4.3.7",
    "nodemailer": "^6.7.7",
    "protobufjs": "^7.0.0",
    "rxjs": "^7.5.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.2",
    "@loopback/eslint-config": "^13.0.2",
    "@loopback/testlab": "^5.0.2",
    "@types/adm-zip": "^0.5.0",
    "@types/axios": "^0.14.0",
    "@types/dot": "^1.1.5",
    "@types/form-data": "^2.5.0",
    "@types/lodash": "^4.14.168",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^14.18.21",
    "eslint": "^8.19.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
