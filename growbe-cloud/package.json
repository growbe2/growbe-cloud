{
  "name": "growbe-cloud",
  "version": "0.0.0",
  "description": "Cloud server for Growbe",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "lb-mocha \"dist/__tests__\"",
    "test:dev": "npm run build && lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "docker:build": "docker build -f ./docker/growbe-cloud.Dockerfile -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-cloud:$VERSION --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
    "docker:push": "docker push docker.pkg.github.com/growbe2/growbe-cloud/growbe-cloud:$VERSION",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start:cloud": "node -r source-map-support/register ./dist/growbe-cloud.js",
    "start:watcher": "node -r source-map-support/register ./dist/growbe-watcher.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "William Quintal <wquintal@berlingoqc.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@berlingoqc/lb-extensions": "^1.1.2",
    "@berlingoqc/sso": "^1.1.2",
    "@growbe2/growbe-pb": "^1.1.5-SNAPSHOT-8",
    "@loopback/boot": "^4.0.1",
    "@loopback/core": "^3.0.1",
    "@loopback/repository": "^4.0.1",
    "@loopback/rest": "^11.0.1",
    "@loopback/rest-explorer": "^4.0.1",
    "@loopback/security": "^0.6.1",
    "@loopback/service-proxy": "^4.0.1",
    "async-mqtt": "^2.6.1",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^6.1.0",
    "loopback-connector-postgresql": "^5.4.0",
    "mqtt": "^4.3.4",
    "protobufjs": "^6.11.2",
    "rxjs": "^7.5.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.1",
    "@loopback/eslint-config": "^12.0.1",
    "@loopback/testlab": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^10.17.60",
    "eslint": "^8.6.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.5.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
