{
  "name": "growbe-portal",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build",
    "prebuild": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build dashboard && ng build greenhouse",
    "prebuild:prod": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build dashboard && ng build greenhouse",
    "build:prod": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build --configuration production",
    "postbuild:prod": "mv dist/geenhouse/bundles/growbe2-growbe-greenhouse-module.umd.min.js dist/growbe-portal/assets/greenhouse.umd.js",
    "greenhouse": "ng build greenhouse && mv dist/geenhouse/bundles/growbe2-growbe-greenhouse-module.umd.min.js src/assets/greenhouse.umd.js",
    "e2e": "ng e2e",
    "lint:check": "eslint ./src/app",
    "lint:write": "prettier --write ./src && eslint --fix ./src/app/",
    "ut:dev": "ng test --watch=true growbe-portal",
    "ut:run": "ng test --watch=false --browsers=ChromeHeadless growbe-portal",
    "ut:ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox growbe-portal",
    "ut:coverage": "ng test --code-coverage",
    "postinstall": "ngcc",
    "postci": "ngcc",
    "update:node-editor": "npm install @growbe2/node-editor && cp ./node_modules/@growbe2/node-editor/node-editor* ./src/assets/node-editor/",
    "docker:build": "docker build -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal:$VERSION --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
    "docker:build:stage": "docker build -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal-ci --target $STAGE --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
    "docker:push": "docker push docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal:$VERSION"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.4",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.4",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@angular/service-worker": "^15.0.4",
    "@auth0/angular-jwt": "^5.0.2",
    "@berlingoqc/auth": "^2.2.32",
    "@berlingoqc/fuse": "^1.1.10",
    "@berlingoqc/fuse-extra": "^1.1.10",
    "@berlingoqc/ngx-autoform": "^2.2.32",
    "@berlingoqc/ngx-autotable": "^2.2.32",
    "@berlingoqc/ngx-common": "^2.2.32",
    "@berlingoqc/ngx-loopback": "^2.2.32",
    "@berlingoqc/ngx-notification": "^2.2.32",
    "@berlingoqc/ngx-pwa": "^2.2.31",
    "@growbe2/ngx-cloud-api": "1.2.5",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/dragula": "^3.8.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "async-mqtt": "^2.6.2",
    "buffer": "^6.0.3",
    "flv.js": "^1.6.2",
    "moment": "^2.29.4",
    "mqtt": "^4.3.7",
    "mqtt-pattern": "^1.2.0",
    "next-tick": "^1.1.0",
    "ngx-cookie-service": "^15.0.0",
    "ngx-markdown": "^15.0.0",
    "ngx-permissions": "^13.0.1",
    "object-hash": "^3.0.0",
    "perfect-scrollbar": "^1.5.5",
    "process": "^0.11.10",
    "rxjs": "~7.4.0",
    "systemjs": "^0.21.3",
    "tslib": "^2.0.0",
    "url": "^0.11.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "^15.0.5",
    "@angular/compiler-cli": "^15.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/protobufjs": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.0.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}
