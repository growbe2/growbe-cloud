{
    "name": "growbe-portal",
    "version": "0.0.0",
    "scripts": {
        "start": "ng serve",
        "build": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build",
        "prebuild": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build dashboard && ng build greenhouse",
        "prebuild:prod": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build dashboard && ng build greenhouse",
        "build:prod": "NODE_OPTIONS=\"--max-old-space-size=1024\" ng build --prod",
        "postbuild:prod": "mv dist/geenhouse/bundles/growbe2-growbe-greenhouse-module.umd.min.js dist/growbe-portal/assets/greenhouse.umd.js",
        "greenhouse": "ng build greenhouse && mv dist/geenhouse/bundles/growbe2-growbe-greenhouse-module.umd.min.js src/assets/greenhouse.umd.js",
        "e2e": "ng e2e",
        "lint:check": "eslint ./src/app",
        "lint:write": "prettier --write ./src && eslint --fix ./src/app/",
        "ut:dev": "ng test --watch=true growbe-portal",
        "ut:run": "ng test --watch=false --browsers=ChromeHeadless growbe-portal",
        "ut:ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox growbe-portal",
        "ut:coverage": "ng test --code-coverage",
        "postinstall": "ngcc",
        "postci": "ngcc",
        "update:node-editor": "npm install @growbe2/node-editor && cp ./node_modules/@growbe2/node-editor/node-editor* ./src/assets/node-editor/",
        "docker:build": "docker build -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal:latest --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
        "docker:build:stage": "docker build -t docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal-ci --target $STAGE --build-arg GITHUB_ACCESS_TOKEN=$GITHUB_ACCESS_TOKEN .",
        "docker:push": "docker push docker.pkg.github.com/growbe2/growbe-cloud/growbe-portal"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.1.1",
        "@angular/cdk": "^11.1.1",
        "@angular/common": "~11.1.1",
        "@angular/compiler": "~11.1.1",
        "@angular/core": "~11.1.1",
        "@angular/flex-layout": "^11.0.0-beta.33",
        "@angular/forms": "~11.1.1",
        "@angular/material": "^11.1.1",
        "@angular/platform-browser": "~11.1.1",
        "@angular/platform-browser-dynamic": "~11.1.1",
        "@angular/router": "~11.1.1",
        "@angular/service-worker": "~11.1.1",
        "@auth0/angular-jwt": "^5.0.2",
        "@berlingoqc/auth": "^2.0.4",
        "@berlingoqc/fuse": "^1.1.0",
        "@berlingoqc/fuse-extra": "^1.1.0",
        "@berlingoqc/ngx-autoform": "^2.1.5",
        "@berlingoqc/ngx-autotable": "^2.1.5",
        "@berlingoqc/ngx-common": "^2.1.5",
        "@berlingoqc/ngx-loopback": "^2.1.5",
        "@berlingoqc/ngx-notification": "^2.1.5",
        "@berlingoqc/ngx-pwa": "^2.1.5",
        "@growbe2/growbe-pb": "0.0.17",
        "@growbe2/ngx-cloud-api": "^1.1.5-cf8658da",
        "@growbe2/node-editor": "0.0.2",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "@swimlane/dragula": "^3.8.0",
        "@swimlane/ngx-charts": "^17.0.0",
        "@types/protobufjs": "^6.0.0",
        "async-mqtt": "^2.6.1",
        "buffer": "^6.0.3",
        "flv.js": "^1.5.0",
        "moment": "^2.29.1",
        "mqtt": "^4.2.6",
        "mqtt-pattern": "^1.2.0",
        "next-tick": "^1.1.0",
        "ngx-cookie-service": "^11.0.2",
        "ngx-permissions": "^8.1.1",
        "object-hash": "^2.1.1",
        "perfect-scrollbar": "^1.5.0",
        "process": "^0.11.10",
        "protobufjs": "^6.10.2",
        "rollup": "^2.36.1",
        "rxjs": "~6.6.0",
        "systemjs": "^0.21.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1101.1",
        "@angular-eslint/builder": "1.2.0",
        "@angular-eslint/eslint-plugin": "1.2.0",
        "@angular-eslint/eslint-plugin-template": "1.2.0",
        "@angular-eslint/schematics": "1.2.0",
        "@angular-eslint/template-parser": "1.2.0",
        "@angular/cli": "~11.1.1",
        "@angular/compiler-cli": "~11.1.1",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.3.0",
        "@typescript-eslint/parser": "4.3.0",
        "codelyzer": "^6.0.0",
        "eslint": "^7.6.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.2.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.0.0",
        "prettier": "^2.2.1",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}
